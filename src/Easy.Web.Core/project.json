{
  "version": "0.1.0-*",
  "authors": [ "Nima Ara" ],
  "title": "Easy Web",
  "name": "Easy.Web",
  "description": "A fast, lean and opinionated web framework based on the Microsoft ASP.NET Core.",
  "copyright": "2017 Nima Ara",
  "packOptions": {
    "copyright": "2017 Nima Ara",
    "owners": [ "Nima Ara" ],
    "licenseUrl": "https://github.com/NimaAra/Easy.Web/blob/master/LICENSE",
