    "requireLicenseAcceptance": false,
    "summary": "A fast, lean and opinionated web framework based on the Microsoft ASP.NET Core.",
    "tags": [ "Easy", "Web", "HTTP", "Server", "WebAPI", "Core", "ASPNetCore" ],
    "releaseNotes": "",
    "files": "bin\\Release\\net451\\Easy.Web.Core.xml",
